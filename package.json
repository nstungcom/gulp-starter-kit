{
  "name": "@nstungcom/gulp-starter-kit",
  "description": "Gulp Starter Kit for front-end developers.",
  "version": "4.0.0",
  "version_short": "4.0",
  "author": "Son Tung Ngo <nstungcom@gmail.com>",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/nstungcom/gulp-starter-kit/issues",
    "email": "nstungcom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "dev": "npm start",
    "start": "run-p eleventy-watch gulp-dev",
    "build": "run-s clean prettier-check eleventy-build gulp-build",
    "css": "run-s clean lint-css eleventy-build gulp-build-js gulp-build-css",
    "lint-css": "stylelint \"src/**/*.{css,scss}\"",
    "fix-css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "js": "run-s gulp-build-js",
    "lint-js": "eslint .",
    "fix-js": "eslint . --fix",
    "clean": "gulp clean",
    "eleventy-watch": "eleventy --watch",
    "eleventy-build": "cross-env NODE_ENV=production eleventy",
    "gulp-dev": "gulp",
    "gulp-build": "cross-env NODE_ENV=production gulp build",
    "gulp-build-css": "cross-env NODE_ENV=production gulp css",
    "gulp-build-js": "cross-env NODE_ENV=production gulp js",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "prepare": "husky install",
    "test": "npm-run-all prettier-check -p lint-css lint-js"
  },
  "keywords": [
    "gulp-starter-kit",
    "gulp 4",
    "css",
    "sass",
    "eleventy",
    "front-end",
    "web",
    "gulpjs",
    "rollup"
  ],
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@cloudfour/simple-svg-placeholder": "^1.0.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "browser-sync": "^2.26.14",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-eslint7": "^0.3.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^9.0.0",
    "gulp-purgecss": "^4.0.3",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "html-minifier": "^4.0.0",
    "husky": "^6.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-config-standard": "^4.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0"
  }
}
