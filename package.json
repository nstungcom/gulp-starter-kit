  "author": "Son Tung Ngo <nstungcom@gmail.com>",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/nstungcom/gulp-starter-kit/issues",
    "email": "nstungcom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstungcom/gulp-starter-kit.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "npm-run-all start",
    "start": "npm-run-all clean -p eleventy-watch gulp-dev",
    "build": "npm-run-all clean eleventy-build gulp-build",
    "clean": "gulp clean",
    "eleventy-watch": "eleventy --watch",
    "eleventy-build": "eleventy",
    "gulp-dev": "gulp",
    "gulp-build": "cross-env NODE_ENV=production gulp build --production",
    "prepare": "husky install",
    "test": "npm run build"
  },
  "keywords": [
    "gulp-starter-kit",
    "gulp 4",
    "css",
    "sass",
    "eleventy",
    "front-end",
    "web",
    "gulpjs",
    "rollup"
  ],
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "browser-sync": "^2.26.14",
    "core-js": "^3.10.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
